{
  "name": "tanstack-start",
  "private": true,
  "version": "1.0.0",
  "sideEffects": false,
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently -r pnpm:dev:web pnpm:dev:db",
    "dev:web": "vinxi dev",
    "dev:db": "npx convex dev",
    "build": "vinxi build 2>/dev/null & true 'Ignore vinxi build warnings until tanstack start can run without them (need updated guide)'",
    "start": "vinxi start",
    "lint": "eslint --ext .ts,.tsx ./app",
    "prettier:check": "prettier --check ./app/**/* --ignore-unknown",
    "prettier:format": "prettier --write ./app/**/* --ignore-unknown",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clerk/backend": "^1.31.2",
    "@clerk/tanstack-react-start": "^0.14.3",
    "@convex-dev/react-query": "0.0.0-alpha.8",
    "@radix-ui/react-collapsible": "^1.1.9",
    "@radix-ui/react-dialog": "^1.1.12",
    "@radix-ui/react-popover": "^1.1.11",
    "@radix-ui/react-separator": "^1.1.5",
    "@radix-ui/react-slot": "^1.2.0",
    "@tanstack/react-query": "^5.75.0",
    "@tanstack/react-router": "^1.119.0",
    "@tanstack/react-router-devtools": "^1.119.1",
    "@tanstack/react-router-with-query": "^1.119.0",
    "@tanstack/react-start": "^1.119.0",
    "convex": "^1.23.0",
    "convex-helpers": "^0.1.84",
    "lucide-react": "^0.503.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "redaxios": "^0.5.1",
    "svix": "^1.64.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "vinxi": "0.5.6"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.74.7",
    "@tanstack/eslint-plugin-router": "^1.115.0",
    "@types/node": "^22.15.3",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.21",
    "concurrently": "~8.2.2",
    "eslint": "^9.26.0",
    "eslint-plugin-react": "^7.37.5",
    "postcss": "^8.5.3",
    "prettier": "3.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "packageManager": "pnpm@10.10.0"
